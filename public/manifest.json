{
  "manifest_version": 2,
  "name": "Browser Extension Demo",
  "author": "Hu Chen",
  "version": "1.0.1",
  "description": "This is a demo for a tab extension",
  "icons": {
    "src": "favicon.ico",
    "sizes": "64x64 32x32 24x24 16x16",
    "type": "image/x-icon"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"]
}
