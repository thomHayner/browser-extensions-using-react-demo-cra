{
  "manifest_version": 3,
  "name": "Browser Extension Demo",
  "author": "me",
  "version": "1.0.1",
  "description": "This is a demo for a tab extension",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Open the popup",
    "default_icon": "logo192.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
